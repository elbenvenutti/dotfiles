		</keyMapSelect>
		<keyMapSelect mapIndex="8">
			<modifier keys="anyShift caps"/>
		</keyMapSelect>
		<keyMapSelect mapIndex="9">
			<modifier keys="anyShift anyOption command? caps"/>
		</keyMapSelect>
	</modifierMap>
