		</keyMapSelect>
		<keyMapSelect mapIndex="6">
			<modifier keys="command anyOption caps?"/>
		</keyMapSelect>
		<keyMapSelect mapIndex="7">
			<modifier keys="control command? shift? caps? option?"/>
